		<!-- MAIN -->
        {% block index %}{% endblock %}
        {% block tablas %}{% endblock %}
        {% block tablasProduct %}{% endblock %}
        {% block register %}{% endblock %}
        {% block articuloRegister %}{% endblock %}
        {% block edit %}{% endblock %}
        {% block articuloEdit %}{% endblock %}
        {% block tablasPedidos %}{% endblock %}
        {#{% block login %}{% endblock %}#}
 		{# Resto de bloques que necesitamos #}
