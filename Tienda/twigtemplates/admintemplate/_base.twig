			<div id="shadow"></div>
			<div id="box"></div>
		</div>
	</div>
	<!-- WRAPPER -->
	<div id="wrapper">
		<!-- NAVBAR -->
		<nav class="navbar navbar-default navbar-fixed-top">
			<div class="brand">
				<a href="index.twig"><img src="{{twigFolder}}/assets/img/logo.jpg" alt="Perfect Woman Logo" class="img-responsive logo"></a>
			</div>
			<div class="container-fluid">
				<div class="navbar-btn">
					<button type="button" class="btn-toggle-fullwidth"><i class="lnr lnr-arrow-left-circle"></i></button>
				</div>
				<div id="navbar-menu">
					<ul class="nav navbar-nav navbar-right">
						
						<li class="dropdown">
							{#<a href="#" class="dropdown-toggle" data-toggle="dropdown"><span>{{ "Hola ! "}}</span><img src="{{twigFolder}}/assets/img/user.png" class="img-circle" alt="Avatar"> <span>{{user.nombre}}</span> <i class="icon-submenu lnr lnr-chevron-down"></i></a>#}
							<a href="#" class="dropdown-toggle" data-toggle="dropdown"><span>{{user_logged.nombre }}</span> <i class="icon-submenu lnr lnr-chevron-down"></i><img src="{{twigFolder}}/assets/img/user.png" class="img-circle" alt="Avatar"> </a>
							<ul class="dropdown-menu">
								<li><a href="#"><i class="lnr lnr-user"></i> <span> {{user_logged.nombre}}</span></a></li>
