							</ul>
						</li>
					</ul>
				</div>
			</div>
		</nav>
		<!-- END NAVBAR -->
		<!-- LEFT SIDEBAR -->
		<div id="sidebar-nav" class="sidebar">
			<div class="sidebar-scroll">
				<nav>
					<ul class="nav">
						<li><a href="index.twig" class="active"><i class="lnr lnr-home"></i> <span>Inicio</span></a></li>
						{% if admin %}
							<li>
								<a href="#subPages" data-toggle="collapse" class="collapsed"><img class="mi_icon" src="{{twigFolder}}/assets/img/icons/users.png"><span>Usuarios</span> <i class="icon-submenu lnr lnr-chevron-left"></i></a>
